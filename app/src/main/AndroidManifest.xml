<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.edu.xtalent">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.XTalent"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".Menu"
            android:exported="false"
            android:label="@string/title_activity_menu"
            android:theme="@style/Theme.XTalent.NoActionBar" />
        <activity
            android:name=".PaginaRegistro"
            android:exported="false" />
        <activity
            android:name=".PaginaInicio"
            android:exported="false" />
        <activity
            android:name=".PaginaRecuperacion"
            android:exported="false" />
        <activity
            android:name=".PaginaLogin"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>